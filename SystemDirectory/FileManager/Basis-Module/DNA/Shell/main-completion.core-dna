#!/bin/bash
# dna-completion – Bash Tab-Vervollständigung für DNA-Kommandos
# main.core-dna sucht shell cmd .cmd-dna

_dna_completions() {
    local cur
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"

    local cmds=$(cd /Pylovara/SystemDirectory/FileManager/Basis-Module/Logic/Shell && ls *.cmd-dna 2>/dev/null | sed 's/.cmd-dna$//')

    COMPREPLY=( $(compgen -W "$cmds" -- "$cur") )
}
complete -F _dna_completions dna
